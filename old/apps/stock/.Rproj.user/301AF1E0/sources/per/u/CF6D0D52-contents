y = x %>% 
  mutate(INCDTTM = parse_date_time(INCDTTM, orders = c('%m/%d/%Y %H:%M:%S %p'),
                                   tz = 'America/Los_Angeles')) %>% 
  filter(!is.na(INCDTTM)) %>%
  mutate(INCHOUR = hour(INCDTTM), INCMONTH = month(INCDTTM, label = TRUE)) %>%
  group_by(INCHOUR, INCMONTH) %>% 
  summarize(CNT = n())

rownames(prices_monthly)

prices_monthly <- cbind(Row.Names = rownames(prices_monthly), prices_monthly)

library(lubridate)

prices_monthly2 = as.data.frame(prices_monthly)
prices_monthly2$data = rownames(prices_monthly2)
prices_monthly2$day = day(prices_monthly2$data)
hchart(prices_monthly, 'heatmap', hcaes(x = day, y = data, value = BOVA11.SA))
